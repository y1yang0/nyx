func add(a,b){
    return a+b
}

func add3(b){
    return func(){
        return add(3,b)
    }
}
func twice(){
    return func(){
        return 5+5
    }
}

func trible(p){
    original = p

    a =  func(){
       return original
    }
    b = func(){
        return a()+p
    }
    c= func(){
        return b()+p
    }
    return c
}
t = trible(3)
println(t()==9)
println(add(1,2)==3)
a = add3(4)
println(typeof(a)=="closure")
println(a()==7)
q=twice()
println(q()==10)
println(q()==10)
println(q)
func weird(num){
    closure_arr = []
    for(i:range(num)){
        closure_arr += func(){
            return i
        }    
    }
    return closure_arr
}
for(closure:weird(3)){
    println(closure+" produces the result:"+closure())
}