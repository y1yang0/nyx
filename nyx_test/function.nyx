func repeat(a,str){
    i = 0
    while(i<a){
        print(str)
        i = i+1
    }
}

func repeat1(str){
    result = ""
    i = 0
    a = length(str)
    while(i<a){
        result = result+str
        i = i+1
    }
    return result
}

func toStar(str){
    result = ""
    i =0
    while(i<length(str)){
        result = result+"*"
        i = i+1
    }
    return result
}

repeat(10,"greeting!")
print(repeat1("concatenation!"))
print(repeat1("new"))
print(toStar("i come i see i conquer"))

