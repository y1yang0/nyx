func repeat(a,str){
    i = 0
    while(i<a){
        println(str)
        i = i+1
    }
}

func repeat1(str){
    result = ""
    i = 0
    a = length(str)
    while(i<a){
        result = result+str
        i = i+1
    }
    return result
}

func toStar(str){
    result = ""
    i =0
    while(i<length(str)){
        result = result+"*"
        i = i+1
    }
    return result
}

repeat(10,"greeting!")
println(repeat1("concatenation!"))
println(repeat1("new"))
println(toStar("i come i see i conquer"))

func force(){
    i = 0
    while(true){
        if(i<10){
            println("ok")
        }else{
            println("return")
            return i
        }
        i = i+1
    }
}

func force1(){
    i = 0
    while(true){
        if(i<10){
            println("ok")
        }else{
            println("break")
            break
        }
        i = i+1
    }
    while(i<20){
        println("once again")
        i = i +1
    }
    return i
}


force()
res = force1()
println(res)

func upto10(){
    i=0
    while(true){
        if(i==10){
            break
        }
        println("up")
        i = i+1
    }
}
upto10()

func filter_odd(limit)
{
    i = 1
    while(i<limit){
        if(i%2==1){
            i = i+1
            continue
        }
        println(i)
        i = i+1
    }
}

